   </main> {# #page #}
   </div> {# .container-fluid #}
   </div> {# .content #}
   </div> {# .page #}

   <footer role="contentinfo" id="footer">
      <table role="presentation">
         <tr>
            {% if is_debug_active %}
               <td class="right">
                  <span class="copyright">
                     {{ __('%1$s - %2$s')|format(
                           _n('%s second', '%s seconds', execution_time)|format(execution_time),
                           memory_usage|format_binary()
                        ) }}
                  </span>
               </td>
            {% endif %}
         </tr>
      </table>
   </footer>

   {% for js_file in js_files %}
      <script type="text/javascript" src="{{ js_path(js_file) }}"></script>
   {% endfor %}
</body>
</html>
